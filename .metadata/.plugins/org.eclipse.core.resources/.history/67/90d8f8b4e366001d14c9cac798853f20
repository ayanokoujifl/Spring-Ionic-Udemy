package com.ayanokoujifl.cursomc.entities;

public class Produto {

	private Integer id;
	private String nome;
	private Double preco;

	public Produto() {
		// TODO Auto-generated constructor stub
	}

	public Produto(Integer id, String nome, Double preco) {
		super();
		this.id = id;
		this.nome = nome;
		this.preco = preco;
	}
}
